{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
  },
  "references": [
    {"path": "packages/model"}
  ],
  "exclude": [
    "**/node_modules"
  ],
  "include": []
}